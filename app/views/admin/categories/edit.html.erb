<div class="row">
  <div class=".col-xs-3"></div>
  <div class=".col-xs-6">
     
     <%= form_with url: admin_categories_path ,method: :post,model: @category, local: true do |f| %>
    <li>
      <%= f.label :name, 'カテゴリー名' %>
      <br>
      <%= f.text_field :name, class:'form-control' %> 
      <br>
      <%= f.submit '保存', class: 'btn btn-primary btn-block'%>
      
    </li>
  <% end %>
  </div>
  <div class=".col-xs-6"></div>
</div>