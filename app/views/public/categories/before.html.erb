<div class="container px-5 px-sm-0">
 <div class="row">
  <div class="col-md-8">
    
       <h3>カテゴリー：お迎え前</h3>

   
     <table class="table table-hover table-inverse">

 
       <tbody>
       <% @before_questions.each do |before_question| %>
         <tr>
            <td><%=link_to before_question.question_content, question_path(question.id) %>
            <%= before_question.created_at.strftime('%Y/%m/%d') %></td>
         </tr>

       <% end %>
       </tbody>
     </table>
    <%= paginate @before_questions %>
    
    
    </div>
  </div>
</div>