<div class="container px-5 px-sm-0">
 <div class="row">
  <div class="col-md-8">
    
       <h3>カテゴリー：病気・ケガ</h3>

   
     <table class="table table-hover table-inverse">

 
       <tbody>
       <% @illness_questions.each do |illness_question| %>
         <tr>
            <td><%=link_to illness_question.question_content, question_path(illness_question.id) %>
            <%= illness_question.created_at.strftime('%Y/%m/%d') %></td>
         </tr>

       <% end %>
       </tbody>
     </table>
    <%= paginate @illness_questions %>
    
    
    </div>
  </div>
</div>