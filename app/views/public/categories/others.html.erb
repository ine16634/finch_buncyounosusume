<div class="container px-5 px-sm-0">
 <div class="row">
  <div class="col-md-8">
    
       <h3>カテゴリー：その他</h3>

   
     <table class="table table-hover table-inverse">

 
       <tbody>
       <% @others_questions.each do |others_question| %>
         <tr>
            <td><%=link_to others_question.question_content, question_path(others_question.id) %>
            <%= others_question.created_at.strftime('%Y/%m/%d') %></td>
         </tr>

       <% end %>
       </tbody>
     </table>
    <%= paginate @others_questions %>
    
    
    </div>
  </div>
</div>