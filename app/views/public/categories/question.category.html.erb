  <div class="col-md-8">
    <h3>検索結果</h3>

     <table class="table table-hover table-inverse">
       <tbody>
       <% @questions.each do |question| %>
         <tr>
            <td><%=link_to question.question_content, question_path(question.id) %>
            <%= question.created_at.strftime('%Y/%m/%d') %></td>
         </tr>

       <% end %>
       </tbody>
     </table>