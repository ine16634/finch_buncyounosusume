<div class="container px-5 px-sm-0">
 <div class="row">
  <div class="col-md-8">
    
       <h1>Q&A一覧</h1>

   
     <table class="table table-hover table-inverse">

 
       <tbody>
       <% @questions.each do |question| %>
         <tr>
          <td><%=link_to question.question_content, question_path(question.id) %>
            <%= question.created_at.strftime('%Y/%m/%d') %></td>
         </tr>

       <% end %>
       </tbody>
     </table>
    <%= paginate @questions %>
    
    
    </div>
  </div>
</div>